    <owl:NamedIndividual rdf:about="http://www.purl.org/pankos#ALBA">
        <rdf:type rdf:resource="http://www.purl.org/pankos#Synchrotron"/>
        <pankos:hasInstrument rdf:resource="http://www.purl.org/pankos#BOREAS"/>
