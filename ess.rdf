        <dc:description>LMX is an innovative single crystal neutron diffractometer that will provide a high flux cold neutron solution for problems in large molecule chemical and biological structure.</dc:description>
        <pankos:preferredName rdf:datatype="http://www.w3.org/2000/01/rdf-schema#Literal">LMX</pankos:preferredName>
    </owl:NamedIndividual>
    


    <!-- http://www.purl.org/pankos#LOQ -->

    <owl:NamedIndividual rdf:about="http://www.purl.org/pankos#LOQ">
